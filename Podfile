# Uncomment the next line to define a global platform for your project
platform :ios, '9.0'

target 'Armonia Social CERO' do
  # Comment the next line if you're not using Swift and don't want to use dynamic frameworks
  use_frameworks!

  # Pods for Armonia Social CERO
  pod 'MBProgressHUD'
  pod 'Alamofire'
  pod 'Firebase'
  pod 'Firebase/Core'
  pod 'Firebase/Database'
  pod 'Firebase/Messaging'
  pod 'Firebase/Auth'
  pod 'Firebase/Storage'
  pod 'Fabric'
  pod 'Crashlytics'
  pod 'SWRevealViewController'
  pod 'SwiftKeychainWrapper'

  target 'Armonia Social CEROTests' do
    inherit! :search_paths
    # Pods for testing
    pod 'Firebase'
    pod 'Firebase/Core'
    pod 'Firebase/Auth'
  end

  target 'Armonia Social CEROUITests' do
    inherit! :search_paths
    # Pods for testing
  end

end
